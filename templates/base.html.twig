<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</head>
	<body class="bg-black">
		<nav class="border-gray-200 fixed top-0 w-full bg-[#262626]">
			<div class="max-w-screen-2xl flex flex-wrap text-white items-center gap-[10vw] mx-auto p-4">
				<div class="flex flex-col">
					<a href="{{ path("app_main") }}">
						<p class="mb-1 rounded-lg flex gap-2 items-center justify-center">
							<span class="text-3xl leading-none text-blue-800 font-bold uppercase bg-white rounded-lg p-1">Ya
							</span>
							<span class="text-3xl leading-none text-white font-bold uppercase">Concept
							</span>
							<span class="text-3xl leading-none text-red-600 font-bold uppercase bg-white rounded-lg p-1">Interieur</span>
						</p>
						{# <span class="text-3xl leading-none text-[#B3A89A] font-semibold uppercase">Ya-Concept-Interieur</span> #}
					</a>
					<span class="text-md leading-none font-bold uppercase">Cuisine</span>
					<span class="text-md leading-none font-bold uppercase">Salle de bain</span>
					<span class="text-md leading-none font-bold uppercase">Rangement</span>
				</div>
				<button type="button" class="inline-flex items-center p-2 w-10 h-10 justify-center text-sm text-gray-500 rounded-lg md:hidden" id="navbar-button">
					<span class="sr-only">Open main menu</span>
					<svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="#FFF" viewbox="0 0 17 14">
						<path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 1h15 M1 2h15 M1 7h15 M1 13h15"/>
					</svg>
				</button>
				<div class="hidden w-full md:block md:w-auto" id="navbar-div">
					<ul class="font-medium flex flex-col p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:flex-row md:space-x-8 md:mt-0 md:border-0 md:bg-[#262626]">
						<li>
							<a href="{{ path("app_main") }}" class="block py-2 px-3 text-white rounded hover:bg-gray-100 md:hover:bg-white md:border-0 md:hover:text-black md:p-1 md:hover:duration-700 md:duration-700" {# aria-current="page" #}>Accueil</a>
						</li>
						<li>
							<a href="#" class="block py-2 px-3 text-white rounded hover:bg-gray-100 md:hover:bg-white md:border-0 md:hover:text-black md:p-1 md:hover:duration-700 md:duration-700">Services</a>
						</li>
						<li>
							<a href="#" class="block py-2 px-3 text-white rounded hover:bg-gray-100 md:hover:bg-white md:border-0 md:hover:text-black md:p-1 md:hover:duration-700 md:duration-700">Réalisation cuisine</a>
						</li>
						<li>
							<a href="#" class="block py-2 px-3 text-white rounded hover:bg-gray-100 md:hover:bg-white md:border-0 md:hover:text-black md:p-1 md:hover:duration-700 md:duration-700">Réalisation salle de bain</a>
						</li>
						<li>
							<a href="{{ path("app_contact") }}" class="block py-2 px-3 text-white rounded hover:bg-gray-100 md:hover:bg-white md:border-0 md:hover:text-black md:p-1 md:hover:duration-700 md:duration-700">Contact</a>
						</li>
					</ul>
				</div>
			</div>
		</nav>
		{% block body %}{% endblock %}
		<footer class="bg-white rounded-lg shadow m-4">
			<div class="w-full mx-auto max-w-screen-xl p-4 md:flex md:items-center md:justify-between">
				<span class="text-sm text-gray-500 sm:text-center">© 2024
					<a href="{{ path("app_main") }}" class="hover:underline">YaConceptInterieur™</a>. All Rights Reserved.
				</span>
				<ul class="flex flex-wrap items-center mt-3 text-sm font-medium text-gray-500 sm:mt-0">
					<li>
						<a href="#" class="hover:underline me-4 md:me-6">About</a>
					</li>
					<li>
						<a href="#" class="hover:underline me-4 md:me-6">Privacy Policy</a>
					</li>
					<li>
						<a href="#" class="hover:underline me-4 md:me-6">Licensing</a>
					</li>
					<li>
						<a href="#" class="hover:underline">Contact</a>
					</li>
				</ul>
			</div>
		</footer>
		<script>
			let navbarButton = document.getElementById("navbar-button");
let navbarDiv = document.getElementById("navbar-div");

navbarButton.addEventListener("click", () => {
if (getComputedStyle(navbarDiv).display != "none") {
navbarDiv.style.display = "none";
} else {
navbarDiv.style.display = "block";
}
})
		</script>
	</body>
</html>
